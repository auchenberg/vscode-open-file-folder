{
	"name": "vscode-open-file-folder",
	"displayName": "Open a file's folder as Workspace in VS Code",
	"description": "Open a file's folder as Workspace in VS Code",
	"version": "0.0.1",
	"author": "Kenneth Auchenberg",
	"publisher": "auchenberg",
	"galleryBanner": {
		"color": "#37699A",
		"theme": "dark"
	},
	"homepage": "https://github.com/chrisdias/vscode-opennewinstance/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/chrisdias/vscode-opennewinstance.git"
	},
	"bugs": {
		"url": "https://github.com/chrisdias/vscode-opennewinstance/issues"
	},
	"engines": {
		"vscode": "^1.0.0"
	},
	"license": "SEE LICENSE IN LICENSE.md",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.openFileFolder",
				"title": "Open this folder as Workspace"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.openFileFolder",
					"group": "navigation@1"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"vscode": "^0.11.0"
	}
}